@Html.Partial("~/Views/Admin/Index.cshtml")

<div class="col-xs-12 defaultContainer">
    <div class="col-xs-12 removePadding adminContainer">

        <input id="userID" type="hidden" name="NewsID" value="@ViewBag.UserID">
        <input id="newsID" type="hidden" name="NewsID" value="@ViewBag.NewsID">
        <input id="parentID" type="hidden" name="NewsID" value="@ViewBag.ParentID">

        <div style="width:100%; margin: 0 auto" class="tablecontainer">
            <table id="AdminCommentTable" class="stripe hover row-border" data-page-length='50'>
                <thead>
                    <tr style="text-align:right !important">
                        <th>UserName</th>
                        <th>Comment</th>
                        <th>Fakt</th>
                        <th>Fake</th>
                        <th>Reported</th>
                        <th>Active</th>
                        <th>Date</th>
                        <th>Child</th>
                        <th>Edit</th>
                    </tr>
                </thead>
            </table>
        </div>

    </div>
</div>




